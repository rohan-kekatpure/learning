{
    "manifest_version": 2,

    "name": "Axelar",

    "version": "0.2",

    "description": "Axelar visual search",

    "icons": {
        "128": "icon_128.png",
        "48": "icon_48.png",
        "16": "icon_16.png"
    },

    "browser_action": {
        "default_icon": "icon_16.png",
        "default_popup": "popup.html"
    },

    "background":  {
        "scripts": ["event_page.js"],
        "persistent": false
    },

    "permissions": [
        "storage",
        "contextMenus",
        "http://ec2-18-216-11-182.us-east-2.compute.amazonaws.com:7001/*"
    ]

}